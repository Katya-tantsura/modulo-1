{
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 200,
  "window.zoomLevel": 0,
  "telemetry.enableTelemetry": false, 
  "telemetry.enableCrashReporter": false,
  "editor.fontFamily": "'Victor mono', monospace, 'Fira Code', 'Consolas', 'Courier New', 'HÃ¦ck', 'Envy Code R'",
  "editor.letterSpacing": 0,
  "editor.tabSize": 2,
  "editor.cursorSmoothCaretAnimation": true,
  "breadcrumbs.enabled": true,
  "editor.cursorStyle": "line",
  "editor.renderWhitespace": "selection",
  "editor.cursorBlinking": "smooth",
  "editor.smoothScrolling": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.tree.indent": 4,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.colorTheme": "SynthWave '84",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "html.autoClosingTags": true,
  "editor.formatOnSave": true, 
  "editor.formatOnPaste": true, 
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll": true
  },
  "stylelint.enable": true,
  "eslint.enable": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "bracket-pair-colorizer-2.colors": [
    "#fafafa",
    "#64b5f6",
    "#dce775",
    "#fff176",
    "#ffb74d",
    "#ff8a65",
    "#ff5722",
    "#bf360c"
  ],
  "todo-tree.filtering.excludeGlobs": ["node_modules/**"],
  "todo-tree.general.tags": ["TODO", "BUG", "IMPROVE"],
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "check",
      "type": "tag",
      "iconColour": "lightBlue",
      "foreground": "white",
      "background": "#64b5f6",
      "opacity": 50
    },
    "BUG": {
      "icon": "bug",
      "type": "tag",
      "iconColour": "red",
      "background": "#bf360c",
      "foreground": "white"
    },
    "IMPROVE": {
      "type": "tag",
      "iconColour": "yellow",
      "background": "#fff176",
      "foreground": "#524c00"
    }
  },
  "sync.gist": "",
  "npm.enableScriptExplorer": true,
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "vsicons.dontShowNewVersionMessage": true,
  "terminal.integrated.fontFamily": "monospace",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Debian (WSL)": {
      "path": "C:\\Windows\\System32\\wsl.exe",
      "args": ["-d", "Debian"]
    }
  },
  "terminal.integrated.defaultProfile.windows": "Debian (WSL)",
  "liveServer.settings.donotShowInfoMsg": true,
  "security.workspace.trust.untrustedFiles": "open",
  "color-highlight.languages": [

    "*"
  ],
  "color-highlight.matchRgbWithNoFunction": false,


  "emmet.includeLanguages": {
    "vue": "html",
    "html": "html",
    "javascript": "html"
  }

}